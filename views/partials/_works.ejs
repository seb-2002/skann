<% if (category.title==="Works" || category.title==="CrÃ©ation" ) { %>

<% for (let work in category) { %>

<% if (work !=="title" && work !=="ticker" && work !=="href" ) { %>

<div class="show-box">

  <div class="flex-column work-flex">
    <% if (category[work].href_ex) { %>
    <a target="_blank" href="<%=category[work].href_ex%>">
      <p class="underline subheading"><%=category[work].title%></p>
    </a>

    <% } else if (category[work].href_isHidden) { %>
    <p class="underline subheading"><%=category[work].title%></p>
    <% } else { %>

    <a href="<%=category.href%><%=work%>">
      <p class="underline subheading"><%=category[work].title%></p>
    </a>

    <%  } %>



    <p class="no-text-decoration bold">

      <%=category[work].year%>
    </p>
    <p class="credits credits-works">
      <%=category[work].credits%>
    </p>

  </div>
  </p>
  <% if (category[work].src) { %>
  <% if (category[work].href_ex) { %>
  <a target="_blank" href="<%=category[work].href_ex%>">
    <img class="works-img" src="<%=category[work].src%>" alt="">
  </a>
  <% } else if (category[work].href_isHidden) { %>
  <img class="works-img" src="<%=category[work].src%>" alt="">
  <% } else { %>
  <a href="<%=category.href%><%=work%>">
    <img class="works-img" src="<%=category[work].src%>" alt="">
  </a>

  <%  } %>
  <% } %>
</div>


<% } else { %>

<% } %>

<% } %>

<% } %>